// https://leetcode.com/problems/super-pow

class Solution:
    def superPow(self, a, b):
        if a == 0 or a == 1:
            return a    
        b = [str(num) for num in b]
        b = int(''.join(b))
        res = 1
        while b:
            if b & 1:
                res = res * a
            a = a * a
            b = b >> 1
        return res