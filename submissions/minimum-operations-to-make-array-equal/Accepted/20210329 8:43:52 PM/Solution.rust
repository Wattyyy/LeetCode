// https://leetcode.com/problems/minimum-operations-to-make-array-equal

impl Solution {
    pub fn min_operations(n: i32) -> i32 {
        let mut ret;
        if n % 2 == 0{
            ret = (n / 2) * (n / 2);
        }
        else{
            let m = (n - 1) / 2;
            ret = m * (m + 1);
        }
        ret
    }
}