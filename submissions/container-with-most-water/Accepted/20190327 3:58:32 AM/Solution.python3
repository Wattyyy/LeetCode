// https://leetcode.com/problems/container-with-most-water

class Solution:
    def maxArea(self, height):
        j, i = len(height)-1, 0
        ans = 0
        
        while j > i :
            ans = max(ans, min(height[j], height[i]) * (j - i))
            if height[j]<height[i]:
                j -= 1
            else:
                i += 1
        
        return ans