// https://leetcode.com/problems/jewels-and-stones

from collections import defaultdict
class Solution:
    def numJewelsInStones(self, J, S):
        jewels = set()
        for j in J:
            jewels.add(j)

        d = defaultdict(int)
        for s in S:
            if s in jewels:
                d[s] += 1
        ans = 0
        for key in d:
            ans += d[key]
        return ans

        
            
        