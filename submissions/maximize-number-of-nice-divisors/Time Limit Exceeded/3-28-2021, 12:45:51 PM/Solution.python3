// https://leetcode.com/problems/maximize-number-of-nice-divisors

class Solution:
    def maxprod(self, n):
        MOD = 10 ** 9 + 7
        if n == 2:
            return 2
        elif n == 3:
            return 3
        res = 1
        while (n > 4):
            n -= 3
            res *= 3
            res = res % MOD
        return n * res % MOD


    def maxNiceDivisors(self, primeFactors: int) -> int:
        res = self.maxprod(primeFactors)
        return res
   