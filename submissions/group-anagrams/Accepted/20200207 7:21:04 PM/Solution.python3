// https://leetcode.com/problems/group-anagrams

from collections import defaultdict
class Solution:
    def groupAnagrams(self, strs):
        d = defaultdict(list)
        for string in strs:
            sorted_string = "".join(sorted(string))
            d[sorted_string].append(string)
        ans = []
        for key in d:
            ans.append(d[key])
        return ans
