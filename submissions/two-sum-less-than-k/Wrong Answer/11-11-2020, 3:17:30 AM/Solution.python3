// https://leetcode.com/problems/two-sum-less-than-k

from bisect import bisect_right
class Solution:
    def twoSumLessThanK(self, A, K):
        if len(A) <= 1:
            return -1
        A = sorted(A)
        ans = -1
        for i, v in enumerate(A):
            res_idx = bisect_right(A, K - v)
            if i < res_idx:
                ans = max(ans, v + A[res_idx - 1])
        return ans 

            
        