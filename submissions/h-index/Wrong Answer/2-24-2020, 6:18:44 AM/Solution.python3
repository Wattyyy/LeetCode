// https://leetcode.com/problems/h-index

class Solution:
    def hIndex(self, citations):
        if not citations:
            return 0
        l, r = 0, len(citations)
        while l < r:
            h = r + (l - r) // 2
            cnt = 0
            for i in range(len(citations)):
                if citations[i] >= h:
                    cnt += 1
            if cnt <= h:
                r = h
            else:
                l = h + 1
        return r



        

        
        