// https://leetcode.com/problems/sort-array-by-parity

from collections import deque
class Solution:
    def sortArrayByParity(self, A: List[int]) -> List[int]:
        res = deque([])
        for num in A:
            if num % 2:
                res.append(num)
            else:
                res.appendleft(num)

        return res