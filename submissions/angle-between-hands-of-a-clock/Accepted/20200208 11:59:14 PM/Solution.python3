// https://leetcode.com/problems/angle-between-hands-of-a-clock

class Solution:
    def angleClock(self, hour, minutes):
        if hour == 12:
            hour = 0
        short_rad = hour * 30 + (minutes / 60) * 30
        long_rad = (minutes / 60) * 360
        ans1 = abs(short_rad - long_rad)
        ans2 = 360 - ans1
        return min(ans1, ans2)
        