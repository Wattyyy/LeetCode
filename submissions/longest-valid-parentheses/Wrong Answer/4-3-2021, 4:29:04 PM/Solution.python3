// https://leetcode.com/problems/longest-valid-parentheses

class Solution:
    def longestValidParentheses(self, s: str) -> int:
        st = []
        for char in s:
            if len(st) == 0:
                st.append(char)
            elif st[-1] == '(' and char == ')':
                st.pop()
            else:
                st.append(char)
        return len(s) - len(st)