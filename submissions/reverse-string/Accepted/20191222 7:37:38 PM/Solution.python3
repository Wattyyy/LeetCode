// https://leetcode.com/problems/reverse-string

class Solution:
    def reverseString(self, s):
        """
        Do not return anything, modify s in-place instead.
        """
        if not s:
            return []
        def helper(lp, rp):
            while lp<rp:
                s[lp], s[rp] = s[rp], s[lp]
                lp += 1
                rp -= 1
        return helper(0, len(s)-1)
        
        