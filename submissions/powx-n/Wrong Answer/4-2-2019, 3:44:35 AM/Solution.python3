// https://leetcode.com/problems/powx-n

class Solution:
    def myPow(self, x, n):
        if n==0:
            return 1
        
        elif n>0:
            ans = x
            for _ in range(n-1):
                ans = ans*x
            return ans
    
        else:
            ans = 1
            for _ in range(n):
                ans = ans / x
                return ans