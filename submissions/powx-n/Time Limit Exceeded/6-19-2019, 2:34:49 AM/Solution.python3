// https://leetcode.com/problems/powx-n

class Solution:
    def myPow(self, x, n): 
        if n==0:
            return 1
        ans = 1 
        for _ in range(abs(n)):
            ans *= x
        if n<0:
            return float(1/ans)
        else:
            return ans 

s = Solution()

print(s.myPow(2.000, -2))

        
            
        