// https://leetcode.com/problems/jump-game-ii

class Solution(object):
    def jump(self, nums):
        N = len(nums)
        end = 0 + nums[0]
        new_end = 0 + nums[0]
        ans = 0
        for i in range(1, N):
            new_end = max(new_end, i+nums[i])
            if i == end:
                ans += 1
                end = new_end
        return ans