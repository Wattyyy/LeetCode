// https://leetcode.com/problems/jump-game-ii

class Solution(object):
    def jump(self, nums):
        N = len(nums)
        dist = [float("inf") for _ in range(N)]
        dist[0] = 0
        for i in range(N):
            step = nums[i]
            for s in range(1, step+1):
                if N <= i+s:
                    break
                dist[i+s] = min(dist[i+s], dist[i] + 1)
        return dist[-1]