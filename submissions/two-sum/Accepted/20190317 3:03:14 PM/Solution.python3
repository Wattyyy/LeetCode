// https://leetcode.com/problems/two-sum

class Solution:
    def twoSum(self, nums, target):
        
        int_map = {}
        n = len(nums)
        
        for i in range(n):
            if nums[i] in int_map.keys():
                ans = [i, int_map[nums[i]]]
                return sorted(ans)
            int_map[target-nums[i]] = i