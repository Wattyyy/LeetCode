// https://leetcode.com/problems/two-sum

class Solution:
    def twoSum(self, nums, target):
        n = len(nums)
        for i in range(n):
            if target<nums[i]:
                continue
            else:
                x = target - nums[i]
                if x in nums and nums.index(x)!=i:
                    return [i, nums.index(x)]
