// https://leetcode.com/problems/two-sum

class Solution:
    def twoSum(self, nums, target):
        dic = {}
        for item in nums:
            if item not in dic.keys():
                dic[target - item] = item
            else:
                ind1 = nums.index(item)
                ind2 = nums.index(target-item)
                return sorted([ind1, ind2])