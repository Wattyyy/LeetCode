// https://leetcode.com/problems/unique-paths

def combination(n, r):
    comb = {}
    if r==0 or r==n:
        return 1
    comb[(n, 1)] = n
    else:
        for i in range(1, r):
            comb[(n, i+1)] = comb[(n, i)] * (n-i)/(i+1)
        return comb[(n, r)]
 
class Solution:
    def uniquePaths(self, m, n):
        k = max(m-1, n-1)
        l = m + n -2
        ans = combination(l, k)
        return int(round(ans,0))
    