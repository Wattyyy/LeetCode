// https://leetcode.com/problems/validate-binary-search-tree

# Definition for a binary tree node.
# class TreeNode:
#     def __init__(self, x):
#         self.val = x
#         self.left = None
#         self.right = None

class Solution:
    def isValidBST(self, root: TreeNode) -> bool:
        if self.check_nodes(root)==False:
            return False
        else:
            return True
        
    def check_nodes(self, node):
        try:
            left_val = node.left.val
            right_val = node.right.val
            if not ( left_val<node.val and node.val<right.val ):
                return False
            else:
                self.check_nodes(node.left)
                self.check_nodes(node.right)
        except:
            return  
        