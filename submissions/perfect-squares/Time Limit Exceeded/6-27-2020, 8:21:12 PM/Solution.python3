// https://leetcode.com/problems/perfect-squares

from math import sqrt
class Solution:
    def numSquares(self, n):
        dp = [float('inf')] * (n + 1)
        dp[0] = 0
        i = 1
        while i * i <= n:
            dp[i * i] = 1
            i += 1
            
        for i in range(1, n+1):
            j = 1
            while j ** 2 <= i:
                dp[i] = min(dp[i], dp[i - (j ** 2)] + 1)
                j += 1
        return dp[-1]
