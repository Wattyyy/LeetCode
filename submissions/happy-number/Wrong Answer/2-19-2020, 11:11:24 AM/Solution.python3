// https://leetcode.com/problems/happy-number

class Solution:
    def isHappy(self, n):
        N = str(n)
        L = len(N)
        d_sum = 0
        for i in range(L):
            d_sum += int(N[i]) ** 2
        
        if d_sum == 0:
            return False
        elif d_sum % 9 == 0:
            return False
        else:
            return (d_sum % 9 == 1)