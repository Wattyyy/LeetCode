// https://leetcode.com/problems/longest-substring-without-repeating-characters

class Solution:
    def lengthOfLongestSubstring(self, s):
        ans = 0
        temp = []
        if s ==" ":
            return 0
        
        for i in s:
            if i in temp:
                ans = max(ans, len(temp))
                temp.append(i)
                temp = temp[temp.index(i)+1:]
            else:
                temp.append(i)
        
        return ans
                