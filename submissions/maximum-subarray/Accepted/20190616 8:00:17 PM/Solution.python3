// https://leetcode.com/problems/maximum-subarray

class Solution:
    def maxSubArray(self, nums):
        cur_sum = -float("inf")
        max_so_far = -float("inf")
        
        for num in nums:
            cur_sum += num
            cur_sum = max(cur_sum, num)
            max_so_far = max(cur_sum, max_so_far)
            
        return max_so_far
            