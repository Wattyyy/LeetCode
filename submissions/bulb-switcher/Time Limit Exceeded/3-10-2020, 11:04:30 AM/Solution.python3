// https://leetcode.com/problems/bulb-switcher

class Solution:
    def bulbSwitch(self, n):
        ans = [0 for _ in range(n+1)]
        for i in range(1, n+1):
            for j in range(1, n+1):
                if j % i == 0:
                    ans[j] = ans[j] ^ 1
        return sum(ans)

        