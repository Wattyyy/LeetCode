// https://leetcode.com/problems/longest-common-prefix

class Solution:
    def longestCommonPrefix(self, strs):
        if strs==[]:
            return ""
        min_str = min(strs)
        strs.pop(strs.index(min_str))
        ans = ""
        
        for i in range(len(min_str)):
            for item in strs:
                if item[i] != min_str[i]:
                    return ans
            ans += min_str[i]
                
        return ans