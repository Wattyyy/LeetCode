// https://leetcode.com/problems/count-of-smaller-numbers-after-self

from bisect import bisect_left

class Solution:
    def countSmaller(self, nums):
        if not nums:
            return []
        N = len(nums)
        ans = [0 for _ in range(N)]
        for i in range(N):
            if i == N - 1:
                break
            tmp_list = sorted(nums[i+1:])
            ans[i] = bisect_left(tmp_list, nums[i])
        
        return ans
            
        