// https://leetcode.com/problems/2-keys-keyboard

class Solution:
    def minSteps(self, n):
        dp = [float("inf") for _ in range(n+1)]
        dp[0], dp[1] = 0, 0
        for i in range(1, n):
            if 2 * i > n:
                continue
            dp[2 * i] = min(dp[i] + 2, dp[2 * i])
            idx = 2 * i + i
            while idx <= n:
                dp[idx] = min(dp[idx - i] + 1, dp[idx])
                idx += i
        return dp[n]
            
            
            