// https://leetcode.com/problems/k-closest-points-to-origin

class Solution:
    def kClosest(self, points, K):
        dist_list = []
        for x, y in points:
            distance = (x ** 2) + (y ** 2)
            dist_list.append([distance, x, y])
        dist_list = sorted(dist_list)
        ans = []
        for i in range(K):
            x, y = dist_list[i][1], dist_list[i][2]
            ans.append([x, y])      
        return ans